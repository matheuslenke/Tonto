    "rimraf": "^4.1.2",
    "uuid": "^9.0.1",
    "vscode-languageclient": "~9.0.1",
    "vscode-languageserver": "~9.0.1",
    "vscode-languageserver-types": "^3.17.3",
    "vscode-uri": "^3.0.7"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
