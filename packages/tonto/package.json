{
  "name": "tonto-cli",
  "displayName": "Tonto CLI",
  "publisher": "Lenke",
  "author": {
    "name": "Matheus Lenke Coutinho",
    "email": "matheuslenke@gmail.com"
  },
  "type": "module",
  "description": "Tonto is a DSL for creating OntoUML models",
