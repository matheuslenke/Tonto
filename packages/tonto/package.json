    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitest/coverage-c8": "^0.23.2",
    "langium-cli": "^3.0.0",
    "prettier": "^3.2.5",
