{
    "name": "tonto",
    "displayName": "Tonto",
    "publisher": "lenke",
    "description": "Tonto is a DSL for creating OntoUML models",
    "version": "0.3.7",
    "license": "MIT",
    "icon": "data/images/TontoIcon.png",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/matheuslenke/Tonto",
        "directory": "packages/tonto-vscode"
    },
    "engines": {
        "vscode": "^1.67.0",
        "node": ">=16"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension/main.cjs",
    "contributes": {
        "themes": [
            {
                "label": "Tonto Light",
                "uiTheme": "vs",
                "path": "./src/themes/Tonto-color-theme.json"
            },
            {
                "label": "Tonto Dark",
                "uiTheme": "vs",
                "path": "./src/themes/Tonto-dark-color-theme.json"
            }
        ],
        "languages": [
            {
                "id": "tonto",
                "aliases": [
                    "Tonto",
                    "tonto"
                ],
                "extensions": [
                    ".tonto"
                ],
                "configuration": "./data/language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tonto",
                "scopeName": "source.tonto",
                "path": "./syntaxes/tonto.tmLanguage.json"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "tonto.diagram.open",
                    "when": "editorLangId == 'tonto'"
                },
                {
                    "command": "tonto.diagram.fit",
                    "when": "tonto-diagram-focused"
                },
                {
                    "command": "tonto.diagram.center",
                    "when": "tonto-diagram-focused"
                },
                {
                    "command": "tonto.diagram.delete",
                    "when": "tonto-diagram-focused"
                },
                {
                    "command": "tonto.diagram.export",
                    "when": "tonto-diagram-focused"
                }
            ],
            "editor/context": [
                {
                    "command": "tonto.diagram.open",
                    "when": "editorLangId == 'tonto'",
                    "group": "navigation"
                }
            ],
            "editor/title": [
                {
                    "command": "tonto.diagram.open",
                    "when": "editorLangId == 'tonto'",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "tonto.diagram.open",
                    "when": "resourceExtname == '.tonto'",
                    "group": "navigation"
                }
            ]
        },
        "jsonValidation": [
            {
                "fileMatch": "tonto.json",
                "url": "./data/tonto-manifest-schema.json"
            }
        ],
        "customEditors": [
            {
                "viewType": "tonto",
                "displayName": "Tonto Diagram",
                "selector": [
                    {
                        "filenamePattern": "*.tonto"
                    }
                ],
                "priority": "option"
            }
        ],
        "commands": [
            {
                "command": "tonto.diagram.open",
                "icon": "$(symbol-structure)",
                "title": "Open Tonto Diagram",
                "category": "tonto Diagram"
            },
            {
                "command": "tonto.diagram.fit",
                "title": "Fit to Screen",
                "category": "tonto Diagram"
            },
            {
                "command": "tonto.diagram.center",
                "title": "Center selection",
                "category": "tonto Diagram"
            },
            {
                "command": "tonto.diagram.delete",
                "title": "Delete selected element",
                "category": "tonto Diagram"
            },
            {
                "command": "tonto.diagram.export",
                "title": "Export diagram to SVG",
                "category": "tonto Diagram"
            },
            {
                "command": "catCoding.start",
                "title": "Start Cat Coding"
            }
        ],
        "keybindings": [
            {
                "key": "alt+f",
                "mac": "alt+f",
                "command": "tonto.diagram.fit",
                "when": "tonto-diagram-focused"
            },
            {
                "key": "alt+c",
                "mac": "alt+c",
                "command": "tonto.diagram.center",
                "when": "tonto-diagram-focused"
            },
            {
                "key": "alt+e",
                "mac": "alt+e",
                "command": "tonto.diagram.export",
                "when": "tonto-diagram-focused"
            },
            {
                "key": "delete",
                "mac": "delete",
                "command": "tonto.diagram.delete",
                "when": "tonto-diagram-focused"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:tonto",
        "workspaceContains:tonto.json",
        "onWebviewPanel:tonto",
        "onCustomEditor:tonto",
        "onView:tonto",
        "onCommand:tonto.diagram.open"
    ],
    "files": [
        "bin",
        "out",
        "lib",
        "src",
        "node.js",
        "node.d.ts",
        "webview",
        "syntaxes"
    ],
    "exports": "./out/extension/main.cjs",
    "scripts": {
        "copy:schema": "shx cp ../tonto/tonto-manifest-schema.json ./data",
        "copy:webview": "shx cp ../webview/out/* out/",
        "vscode:prepublish": "npm run clean && tsc --noEmit && npm run lint && node esbuild.mjs --minify",
        "package": "vsce package --no-dependencies",
        "publish": "vsce publish --no-dependencies",
        "clean": "rimraf out",
        "lint": "eslint src --ext ts",
        "lint:fix": "eslint src --ext ts --fix",
        "build": "tsc -b tsconfig.json && node ./esbuild.mjs",
        "build:worker": "esbuild --minify ./out/language/main-browser.cjs --bundle --format=iife --outfile=./public/tonto-server-worker.js",
        "build:monarch": "tsc -b tsconfig.monarch.json",
        "build:web": "npm run build && npm run prepare:public && npm run build:worker && node scripts/copy-monaco-assets.mjs",
        "watch": "node ./esbuild.mjs --watch",
        "coverage": "vitest run --coverage",
        "langium:generate": "langium generate",
        "langium:watch": "langium generate --watch",
        "publish:npm": "npm publish --access public",
        "prepare:public": "node scripts/prepare-public.mjs"
    },
    "dependencies": {
        "chalk": "^5.3.0",
        "commander": "^11.0.0",
        "langium": "^3.0.0",
        "monaco-editor": "npm:@codingame/monaco-editor-treemended@1.83.16",
        "monaco-editor-workers": "^0.45.0",
        "monaco-editor-wrapper": "^4.0.2",
        "monaco-languageclient": "^8.1.1",
        "node-fetch-native": "^1.6.2",
        "prettier": "^3.2.5",
        "rimraf": "^4.1.2",
        "sprotty": "^1.2.0",
        "sprotty-vscode": "^1.0.0",
        "tonto-cli": "~0.3.6",
        "tonto-package-manager": "~0.2.9",
        "tonto-sprotty-webview": "file:../webview",
        "uuid": "^9.0.1",
        "vscode-languageclient": "^9.0.1",
        "vscode-languageserver": "^9.0.1",
        "vscode-languageserver-types": "^3.17.3",
        "vscode-messenger": "^0.4.5",
        "vscode-uri": "^3.0.7",
        "web-worker": "^1.3.0"
    },
    "devDependencies": {
        "@types/jest": "^29.2.3",
        "@types/node": "^18.11.17",
        "@types/node-fetch": "^2.6.1",
        "@types/vscode": "^1.63.0",
        "@vitest/coverage-c8": "^0.23.2",
        "esbuild": "^0.20.1",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.8.0",
        "langium-cli": "^3.0.0",
        "prettier": "^2.8.2",
        "shx": "^0.3.4",
        "typescript": "~4.9.5"
    }
}
