module CheckGeneralizationRigidity {
    //  Caso de erro 1
    category Animal
    phaseMixin AdultAnimal

    genset GeneralizationOne {
        general AdultAnimal
        specifics Animal
    }

    // Caso de erro 2
    mixin SocialLeader

    genset GeneralizationTwo {
        general AdultAnimal
        specifics SocialLeader
    }

    // # Caso de erro 3
    category Animal2 specializes AdultAnimal

    // # Caso de erro 4
    mixin SocialLeader2 specializes AdultAnimal
}
