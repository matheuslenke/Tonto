package CoreDatatypes

datatype PhoneNumber {
  countryCode: int [1]
  bodyNumber: int [1]
}

datatype int specializes number

enum EyeColor {
  Blue,
  Green,
  Brown,
  Black
}

datatype Address {
  country: string
  city: string
  postalCode: string
  street: string
  number: string
  complement: string
}
